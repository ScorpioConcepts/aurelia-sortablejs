{
  "name": "aurelia-sortablejs",
  "version": "1.1.0",
  "description": "A plugin to use SortableJS with Aurelia.",
  "keywords": [
    "aurelia",
    "sortablejs",
    "sortable.js"
  ],
  "homepage": "https://github.com/eriklieben/aurelia-sortablejs",
  "bugs": {
    "url": "https://github.com/eriklieben/aurelia-sortablejs/issues"
  },
  "license": "MIT",
  "author": "Erik Lieben <erik.lieben@gmail.com> (https://www.eriklieben.com/)",
  "main": "dist/amd/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eriklieben/aurelia-sortablejs"
  },
  "devDependencies": {
    "aurelia-tools": "^2.0.0",
    "babel": "^6.5.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.11.6",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "conventional-changelog": "2.0.3",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-changed": "^3.2.0",
    "gulp-conventional-changelog": "^2.0.3",
    "gulp-eslint": "^5.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-yuidoc": "^0.1.2",
    "isparta": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jspm": "2.2.3",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-systemjs": "^0.16.0",
    "object.assign": "^4.0.4",
    "phantomjs-prebuilt": "^2.1.10",
    "require-dir": "^1.1.0",
    "run-sequence": "^2.2.1",
    "systemjs": "^2.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "vinyl-paths": "^2.1.0",
    "yargs": "^12.0.2"
  },
  "jspm": {
    "registry": "npm",
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "dependencies": {
      "sortablejs": "^1.4.2"
    },
    "devDependencies": {
      "aurelia-polyfills": "^1.0.0-beta.1.1.0"
    }
  },
  "dependencies": {
    "aurelia-framework": "^1.0.2",
    "sortablejs": "1.7.0"
  },
  "bundledDependencies": [
    "sortablejs"
  ],
  "aurelia": {
    "documentation": {
      "links": [
        {
          "rel": "license",
          "mediaType": "text/plain",
          "title": "The MIT License (MIT)",
          "href": "LICENSE"
        },
        {
          "rel": "version-history",
          "mediaType": "text/markdown",
          "title": "Change Log",
          "href": "doc/CHANGELOG.md"
        }
      ]
    },
    "import": {
      "dependencies": [
        {
          "name": "sortablejs",
          "main": "sortable",
          "path": "../node_modules/aurelia-sortablejs/node_modules/sortablejs/"
        },
        {
          "name": "aurelia-sortablejs",
          "main": "index",
          "path": "../node_modules/aurelia-sortablejs/dist/amd"
        }
      ],
      "tutorial": [
        "1. in main.js add .plugin('aurelia-sortablejs')"
      ]
    }
  }
}
